{
  "name": "alleyoop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "dotenv -e .env.local -e .env -- prisma migrate dev",
    "db:migrate": "dotenv -e .env.local -e .env -- prisma migrate deploy",
    "db:push": "dotenv -e .env.local -e .env -- prisma db push",
    "db:studio": "dotenv -e .env.local -e .env -- prisma studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "dev:db": "docker-compose up -d",
    "setup": "docker-compose up -d && npm run db:push",
    "db:seed": "tsx scripts/seed.ts",
    "dev:queue": "dotenv -e .env.local -e .env -- tsx watch src/server/queue/processor.ts",
    "dev:all": "npm-run-all --parallel dev:db dev dev:queue",
    "generate": "tsx scripts/generate/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@react-pdf/renderer": "^4.3.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "archiver": "^7.0.1",
    "bullmq": "^5.56.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.7.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "openai": "^5.9.2",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "recharts": "^3.1.0",
    "remark": "^15.0.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.6",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.19.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@10.8.2"
}